{
    "version": 2,
    "build": [
        {
            "src": "Giasu/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "app1/static/(.*)",
            "dest": "app1/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Giasu/wsgi.py"
        }
    ]
}